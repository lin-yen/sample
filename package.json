{
  "name": "sample",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node dev-tools/dev-with-git_hash.cjs",
    "predev": "node dev-tools/check-versions.mjs",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "node dev-tools/type-check.cjs",
    "lint": "eslint . --fix",
    "lint:style": "stylelint \"**/*.{css,scss,vue,sass,html}\"",
    "format": "prettier --write src/",
    "release": "release-it",
    "prepare": "husky",
    "build:stats": "cross-env VITE_SHOW_STATS=true npm run build"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "pinia": "^2.2.6",
    "rxjs": "^7.8.1",
    "vue": "^3.5.12",
    "vue-i18n": "^9.14.1",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@release-it/conventional-changelog": "^9.0.2",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.8.7",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/eslint-plugin": "^1.1.7",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.15.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.14.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-vue": "^9.30.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.3.3",
    "release-it": "^17.10.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.80.6",
    "start-server-and-test": "^2.0.8",
    "stylelint": "^16.3.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.2.1",
    "terser": "^5.36.0",
    "typescript": "~5.6.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.10",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-compression2": "^1.3.0",
    "vite-plugin-ejs": "^1.7.0",
